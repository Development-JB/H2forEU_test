 413  e_CostProduction(i_YEAR(YEAR))..
 414      v_CostProduction(YEAR)
 415      =e=
 416      sum((H2_SYSTEM), v_ProductionH2System(YEAR, H2_SYSTEM) * p_production_volume(YEAR, H2_SYSTEM) * p_production_cost(YEAR, H2_SYSTEM))
