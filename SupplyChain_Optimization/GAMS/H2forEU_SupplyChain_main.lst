 385  **********************************
 386  *** OBJECTIVE & COSTS
 387  **********************************
 388  e_Objective..
 389      v_CostSupply
 390      =e=
 391      sum((i_YEAR(YEAR)), v_CostTotal(YEAR))
 392  ;
 393   
 394  *** Cost functions
 395  e_CostTotal(i_YEAR(YEAR))..
 396      v_CostTotal(YEAR)
 397      =e=
 398      + v_CostTransport(YEAR)
 399      + v_CostProduction(YEAR)
 400      + v_CostTotalDummy(YEAR)
 401  ;
 402   
 403  e_CostDummy(i_YEAR(YEAR))..
 404      v_CostTotalDummy(YEAR)
 405      =e=
 406      sum((NODE_IMPORT), v_ProductionDummy(YEAR, NODE_IMPORT)*1000000)
 407  ;
 408   
 409  e_CostProduction(i_YEAR(YEAR))..
 410      v_CostProduction(YEAR)
 411      =e=
 412      sum((H2_SYSTEM), v_ProductionH2System(YEAR, H2_SYSTEM) * p_production_cost(YEAR, H2_SYSTEM))
 413  ;
 414   
 415  e_CostTransport(i_YEAR(YEAR))..
 416      v_CostTransport(YEAR)
 417      =e=
 418      + v_CostTransportNational(YEAR)
 419      + v_CostTransportInternational(YEAR)
 420      + v_CostTransportConversion(YEAR)
 421  *    + sum((PATH, NODE_PRODUCTION, TRANSPORT_NATIONAL, NODE_EXPORT)
 422  *         $(LINK_PATH_NODE_PRODUCTION(PATH,NODE_PRODUCTION) and LINK_PATH_TRANSPORT_NATIONAL(PATH,TRANSPORT_NATIONAL) and LINK_PATH_NODE_EXPORT(PATH,NODE_EXPORT)),
 423  *         v_Path(YEAR, PATH)*p_transport_national_cost_variable(YEAR, TRANSPORT_NATIONAL)+p_transport_national_distance(NODE_PRODUCTION, NODE_EXPORT,TRANSPORT_NATIONAL)*p_transport_national_cost_fixed(YEAR, TRANSPORT_NATIONAL))
 424  *    + sum((PATH, NODE_EXPORT, TRANSPORT_INTERNATIONAL, NODE_IMPORT)
 425  *         $(LINK_PATH_NODE_EXPORT(PATH,NODE_EXPORT)and LINK_PATH_TRANSPORT_INTERNATIONAL(PATH,TRANSPORT_INTERNATIONAL)and LINK_PATH_NODE_IMPORT(PATH,NODE_IMPORT)),
 426  *         v_Path(YEAR, PATH)*p_transport_international_cost_variable(YEAR, TRANSPORT_INTERNATIONAL)+p_transport_international_distance(NODE_EXPORT, NODE_IMPORT,TRANSPORT_INTERNATIONAL)*p_transport_international_cost_fixed(YEAR, TRANSPORT_INTERNATION
      AL))
 427  *    + sum((PATH, TRANSPORT_CONVERSION)
 428  *         $(LINK_PATH_CONVERSION(PATH,TRANSPORT_CONVERSION)),
 429  *         v_Path(YEAR, PATH)*p_transport_conversion_cost(YEAR, TRANSPORT_CONVERSION))
 430  ;
 431   
 432  e_CostTransportNational(i_YEAR(YEAR))..
 433      v_CostTransportNational(YEAR)
 434      =e=
 435      + sum((PATH, NODE_PRODUCTION, TRANSPORT_NATIONAL, NODE_EXPORT)
 436      $(LINK_PATH_NODE_PRODUCTION(PATH,NODE_PRODUCTION) and LINK_PATH_TRANSPORT_NATIONAL(PATH,TRANSPORT_NATIONAL) and LINK_PATH_NODE_EXPORT(PATH,NODE_EXPORT)),
 437      v_Path(YEAR, PATH)*(p_transport_national_cost_variable(YEAR, TRANSPORT_NATIONAL)+p_transport_national_distance(NODE_PRODUCTION, NODE_EXPORT,TRANSPORT_NATIONAL)*p_transport_national_cost_fixed(YEAR, TRANSPORT_NATIONAL)))
 438  ;
 439   
