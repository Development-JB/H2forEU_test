 449      v_CostTransportConversion(YEAR)
 450      =e=
 451      + sum((PATH, TRANSPORT_CONVERSION)
 452      $(LINK_PATH_CONVERSION(PATH,TRANSPORT_CONVERSION)),
 453      v_Path(YEAR, PATH)*p_transport_conversion_cost(YEAR, TRANSPORT_CONVERSION))
 454  ;
