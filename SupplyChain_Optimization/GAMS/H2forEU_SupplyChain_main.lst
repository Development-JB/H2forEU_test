 456  *** General
 457  e_Demand(i_YEAR(YEAR), NODE_IMPORT)..
 458      sum((TRANSPORT_INTERNATIONAL),p_transport_international_import_capacity(YEAR, NODE_IMPORT, TRANSPORT_INTERNATIONAL))
 459      =e=
 460      v_ProductionDummy(YEAR, NODE_IMPORT)
 461      + sum((PATH)$LINK_PATH_NODE_IMPORT(PATH,NODE_IMPORT),v_Path(YEAR, PATH))
